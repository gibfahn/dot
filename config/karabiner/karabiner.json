{
  "description": "Each rule has a description describing what it changes",
  "global": {
    "check_for_updates_on_startup": true,
    "show_in_menu_bar": true,
    "show_profile_name_in_menu_bar": true
  },
  "profiles": [ {
      "complex_modifications": {
        "parameters": {
          "basic.simultaneous_threshold_milliseconds": 50,
          "basic.to_delayed_action_delay_milliseconds": 0,
          "basic.to_if_alone_timeout_milliseconds": 250,
          "basic.to_if_held_down_threshold_milliseconds": 500
        },
        "rules": [ {
            "description": "Caps Lock is Left Shift.",
            "manipulators": [ {
                "from": { "key_code": "caps_lock", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "left_shift" }],
                "type": "basic"
              } ]
          }, {
            "description": "left_shift is escape if alone, left_control otherwise",
            "manipulators": [ {
                "from": { "key_code": "left_shift", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "left_control" }],
                "to_if_alone": [{ "key_code": "escape" }],
                "type": "basic"
              } ]
          }, {
            "description": "Change shift + delete to forward delete",
            "manipulators": [ {
                "from": { "key_code": "delete_or_backspace", "modifiers": { "mandatory": [ "shift" ], "optional": [ "caps_lock" ] } },
                "to": [{ "key_code": "delete_forward" }],
                "type": "basic"
              } ]
          }, {
            "description": "Set f17 to Hyper key (했), mapped to ` or LCtrl in some keyboards below.",
            "manipulators": [ {
                "from": { "key_code": "f17", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "set_variable": { "name": "hyper", "value": 1 } }],
                "to_after_key_up": [{ "set_variable": { "name": "hyper", "value": 0 } }],
                "type": "basic"
              } ]
          }, {
            "description": "Hyper key (했) + Spacebar turn on 했游냜 (Hyper mouse options)",
            "manipulators": [ {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "spacebar", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "set_variable": { "name": "hyper_mouse", "value": 1 } }],
                "to_after_key_up": [{ "set_variable": { "name": "hyper_mouse", "value": 0 } }],
                "type": "basic"
              } ]
          }, {
            "description": "했游냜+h/j/k/l = move mouse up/down/left/right.",
            "manipulators": [ {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "h", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "x": -1536 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "j", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "y": 1536 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "k", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "y": -1536 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "l", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "x": 1536 } }],
                "type": "basic"
              } ]
          }, {
            "description": "했游냜+d/f/g = left/right/middle click.",
            "manipulators": [ {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "d", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "pointing_button": "button1" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "f", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "pointing_button": "button2" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "g", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "pointing_button": "button3" }],
                "type": "basic"
              } ]
          }, {
            "description": "했游냜+y/u/i/o = scroll up/down/left/right.",
            "manipulators": [ {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "y", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "horizontal_wheel": -52 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "u", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "vertical_wheel": -52 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "i", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "vertical_wheel": 52 } }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper_mouse", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "o", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "mouse_key": { "horizontal_wheel": 52 } }],
                "type": "basic"
              } ]
          }, {
            "description": "했+h/j/k/l to Arrows (vim style).",
            "manipulators": [ {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "h", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "left_arrow" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "j", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "down_arrow" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "k", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "up_arrow" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "l", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "right_arrow" }],
                "type": "basic"
              } ]
          }, {
            "description": "했+y/u/i/o to Home/PgDn/PgUp/End",
            "manipulators": [ {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "y", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "home" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "u", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "page_down" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "i", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "page_up" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "o", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "end" }],
                "type": "basic"
              } ]
          }, {
            "description": "했+Key switches to or opens App.",
            "manipulators": [ {
                "description": "했+Enter = Open new Terminal window.",
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "return_or_enter", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -na kitty" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+a = Open IntelliJ IDEA",
                "from": { "key_code": "a", "modifiers": { "optional": [ "option", "command" ] } },
                "to": [{ "shell_command": "open -a 'IntelliJ IDEA'" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+c = Open HipChat",
                "from": { "key_code": "c", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -a HipChat" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+e = Open Finder",
                "from": { "key_code": "e", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open ~" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+f = Open Firefox",
                "from": { "key_code": "f", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -a 'Firefox Nightly'" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+g = Open Google Chrome",
                "from": { "key_code": "g", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -a 'Google Chrome'" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+s = Open Spotify",
                "from": { "key_code": "s", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -a Spotify" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+t = Open the Terminal",
                "from": { "key_code": "t", "modifiers": { "optional": [ "option", "shift", "command" ] } },
                "to": [{ "shell_command": "open -a kitty" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+Shift+x = restart the touch strip",
                "from": { "key_code": "x", "modifiers": { "mandatory": [ "shift" ], "optional": [ "any" ] } },
                "to": [{ "shell_command": "pkill ControlStrip" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+Shift+w = disable then enable wifi",
                "from": { "key_code": "w", "modifiers": { "mandatory": [ "shift" ], "optional": [ "any" ] } },
                "to": [{ "shell_command": "networksetup -setairportpower en0 off; networksetup -setairportpower en0 on" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+w = Open WorkFlowy",
                "from": { "key_code": "w", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "open -a WorkFlowy" }],
                "type": "basic"
              }, {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "description": "했+d = print today's date",
                "from": { "key_code": "d", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "shell_command": "printf `date \"+%Y-%m-%d\"` | pbcopy" },
                  {"key_code": "v", "modifiers": ["command"]}],
                "type": "basic"
              } ]
          }, {
            "description": "했+; = lock screen (darken screen)",
            "manipulators": [ {
                "conditions": [{ "name": "hyper", "type": "variable_if", "value": 1 }],
                "from": { "key_code": "semicolon", "modifiers": { "optional": [ "any" ] } },
                "to": [{ "key_code": "power", "modifiers": [ "left_control", "left_shift" ] }],
